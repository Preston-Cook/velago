{
  "name": "velago",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "seed": "dotenv -e .env.development -- prisma db seed",
    "dev": "dotenv -e .env.development -- next dev --turbo",
    "build": "dotenv -e .env.production -- next build",
    "start": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env.development -- prisma migrate reset",
    "generate": "prisma generate",
    "studio:dev": "dotenv -e .env.development -- prisma studio",
    "studio:prod": "dotenv -e .env.production -- prisma studio",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register seed/seed.ts"
  },
